<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
			http://www.springframework.org/schema/beans 
			http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
			http://www.springframework.org/schema/context 
			http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<bean class = "com.yihuacomputer.fish.web.mock.PersonDataLoader"/>
	<bean class = "com.yihuacomputer.fish.web.mock.MachineDataLoader"/>
	<bean class = "com.yihuacomputer.fish.web.mock.VersionDataLoader"/>
	<bean class = "com.yihuacomputer.fish.web.mock.RetaincardDataLoader"/>
	<bean class = "com.yihuacomputer.fish.web.mock.ParmDataLoader"/>	
	<bean class = "com.yihuacomputer.fish.web.mock.MonitorDataLoader" init-method="init"/>
	<bean class = "com.yihuacomputer.fish.web.mock.FishInit" init-method="init"/>
</beans>
